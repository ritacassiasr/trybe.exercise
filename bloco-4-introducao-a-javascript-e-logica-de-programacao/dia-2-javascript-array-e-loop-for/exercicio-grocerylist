// Utilize o for para imprimir os elementos da lista groceryList com o console.log():

let groceryList = ['Arroz', 'Feijão', 'Alface', 'Melancia'];
for (index =0; index <= 3; index += 1) { // quantas vezes haverá o looping enquanto for verdadeiro
    console.log(groceryList[index])      // o que será impresso, no caso, o conteúdo da array e todos as suas posições
}
